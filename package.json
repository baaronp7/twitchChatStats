{
  "name": "twitch-chat-chart",
  "version": "1.0.0",
  "description": "Charting Twitch Chat",
  "main": "server.js",
  "scripts": {
    "start": "webpack && node server.js",
    "dev": "npm-run-all --parallel watch:*",
    "watch:webpack": "webpack -w",
    "watch:server": "nodemon --ext js,jsx --ignore public/ server.js",
    "build": "npm run sass && webpack --verbose --colors --display-error-details --config webpack.config.js",
    "sass": "node-sass -o public/css src/scss"
  },
  "keywords": [
    "Twitch",
    "Chat",
    "Chart"
  ],
  "author": "Benjamin Penton baaronp7@gmail.com",
  "license": "ISC",
  "dependencies": {
    "async": "^2.5.0",
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-partial": "^0.1.0",
    "mongoose": "^4.10.4",
    "passport": "^0.3.2",
    "passport-twitch": "^1.0.3",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-bars": "^0.1.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-promise": "^0.5.3",
    "tmi.js": "^1.2.1",
    "webpack": "2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-sass": "^1.2.1",
    "load-grunt-tasks": "^3.5.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "react-scripts": "0.4.1",
    "redux": "^3.7.1",
    "webpack": "2.2.1"
  }
}
